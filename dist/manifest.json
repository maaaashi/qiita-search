{
  "manifest_version": 3,
  "name": "Qiita Search",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": [
    "activeTab",
    "contextMenus"
  ],
  "content_scripts": [
    {
      "js": [
        "assets/content-script-loader.content.tsx.c4fa37ae.5a7c70f5.js"
      ],
      "matches": [
        "<all_urls>"
      ],
      "css": [
        "assets/content.tsx.f7605339.css"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "assets/jsx-runtime.3c71404e.js",
        "assets/content.tsx.c4fa37ae.js"
      ],
      "use_dynamic_url": true
    }
  ]
}